#include <iostream>
using namespace std;

int main()
{
   int a;
   
   cout << "Enter a number: ";
   cin >> a;

   cout << "Multiplication table of " << a << ":\n";
   cout << "1*" << a << "=" << a*1 << '\n';
   cout << "2*" << a << "=" << a*2 << '\n';
   cout << "3*" << a << "=" << a*3 << '\n';
   cout << "4*" << a << "=" << a*4 << '\n';
   cout << "5*" << a << "=" << a*5 << '\n';
   cout << "6*" << a << "=" << a*6 << '\n';
   cout << "7*" << a << "=" << a*7 << '\n';
   cout << "8*" << a << "=" << a*8 << '\n';
   cout << "9*" << a << "=" << a*9 << '\n';
   cout << "10*" << a << "=" << a*10 << '\n';
   
    
    return 0;
}
